{
  "name": "mini-meili",
  "type": "module",
  "version": "0.0.0",
  "author": "BeeeQueue <adam@haglund.dev>",
  "license": "MIT",
  "private": true,
  "packageManager": "pnpm@10.7.0",
  "engines": {
    "node": "^22"
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged"
  },
  "nano-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md,svelte}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint src",
    "test": "tsx --test src/**/*.test.ts",
    "typecheck": "svelte-check",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@iconify-json/lucide": "1.2.34",
    "@iconify-json/simple-icons": "1.2.30",
    "@melt-ui/svelte": "0.86.6",
    "es-toolkit": "1.34.1",
    "meilisearch": "0.49.0",
    "mutative": "1.1.0",
    "p-debounce": "4.0.0",
    "svelte": "5.25.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.11.0",
    "@melt-ui/pp": "0.3.2",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tsconfig/strictest": "2.0.5",
    "@tsconfig/svelte": "5.0.4",
    "@unocss/preset-icons": "65.5.0",
    "@unocss/preset-uno": "65.5.0",
    "@unocss/preset-web-fonts": "65.5.0",
    "@unocss/reset": "65.5.0",
    "@unocss/transformer-directives": "65.5.0",
    "@unocss/vite": "65.5.0",
    "eslint": "9.23.0",
    "eslint-plugin-svelte": "3.4.1",
    "nano-staged": "0.8.0",
    "prettier": "3.5.3",
    "prettier-plugin-svelte": "3.3.3",
    "prettier-plugin-tailwindcss": "0.6.11",
    "simple-git-hooks": "2.12.1",
    "svelte-check": "4.1.5",
    "typescript": "5.8.2",
    "vite": "6.2.4",
    "vite-plugin-pwa": "0.21.2"
  },
  "pnpm": {
    "overrides": {
      "indent-string": "npm:@socketregistry/indent-string@^1",
      "is-core-module": "npm:@socketregistry/is-core-module@^1",
      "object-assign": "npm:@socketregistry/object-assign@^1",
      "path-parse": "npm:@socketregistry/path-parse@^1",
      "safe-buffer": "npm:@nolyfill/safe-buffer@^1",
      "string.prototype.matchall": "npm:@nolyfill/string.prototype.matchall@^1"
    }
  }
}
