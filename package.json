    "meilisearch": "0.50.0",
    "mutative": "1.1.0",
    "p-debounce": "4.0.0",
    "svelte": "5.28.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.2.0",
    "@melt-ui/pp": "0.3.2",
    "@sveltejs/vite-plugin-svelte": "5.0.3",
    "@tsconfig/strictest": "2.0.5",
    "@tsconfig/svelte": "5.0.4",
    "@unocss/preset-icons": "66.0.0",
    "@unocss/preset-uno": "66.0.0",
