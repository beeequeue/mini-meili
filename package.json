  "scripts": {
    "build": "vite build",
    "dev": "vite",
    "lint": "eslint src",
    "test": "tsx --test src/**/*.test.ts",
    "typecheck": "svelte-check",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@iconify-json/lucide": "1.2.39",
    "@iconify-json/simple-icons": "1.2.33",
