    "@unocss/vite": "66.0.0",
    "eslint": "9.33.0",
    "eslint-plugin-svelte": "3.11.0",
    "nano-staged": "0.8.0",
    "prettier": "3.6.2",
    "prettier-plugin-svelte": "3.4.0",
    "prettier-plugin-tailwindcss": "0.6.14",
    "simple-git-hooks": "2.13.1",
    "svelte-check": "4.1.6",
    "typescript": "5.8.3",
    "vite": "npm:rolldown-vite@6.3.5",
    "vite-plugin-pwa": "1.0.0"
  }
