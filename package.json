    "nano-staged": "0.9.0",
    "prettier": "3.8.1",
    "prettier-plugin-svelte": "3.4.1",
    "prettier-plugin-tailwindcss": "0.7.2",
    "simple-git-hooks": "2.13.1",
    "svelte-check": "4.3.6",
    "typescript": "5.9.3",
