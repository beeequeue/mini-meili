  "private": true,
  "packageManager": "pnpm@10.14.0",
  "engines": {
    "node": "^24"
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/nano-staged"
  },
  "nano-staged": {
    "*.{js,jsx,ts,tsx,json,json5,yaml,yml,css,scss,md,svelte}": [
      "prettier --experimental-cli --write"
