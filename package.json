    "p-debounce": "4.0.0",
    "svelte": "5.51.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "5.4.1",
    "@melt-ui/pp": "0.3.2",
    "@sveltejs/vite-plugin-svelte": "6.2.4",
    "@tsconfig/strictest": "2.0.8",
    "@tsconfig/svelte": "5.0.6",
